{
    "root": true,
    "extends": ["../../.eslintrc"],
    "ignorePatterns":[
        "test/fixtures/appWithStorybook",
        "test/fixtures/heavyStorybook",
        "test/fixtures/singleStorybook",
        "test/fixtures/storybookCSF",
        "test/fixtures/storybook-versions",
        "test/fixtures/accessibilityStorybook",
        "test/fixtures/crossOriginIframeStorybook",
        "test/fixtures/jsLayoutStorybook",
        "test/fixtures/jsLayoutStorybookLocal",
        "test/fixtures/waitBeforeCapture",
        "test/fixtures/customFont",
        "test/fixtures/appWithCustomHttpHeaders",
        "test/fixtures/fakeIE",
        "test/fixtures/globalQueryParams",
        "test/fixtures/zeroStoriesRetry",
        "test/fixtures/perComponentStorybook",
        "dist",
        "logs",
        "test/fixtures/storybook-storyStore7",
        "test/fixtures/storybook-with-errors-with-version/",
        "test/fixtures/floatingRegionStorybook",
        "test/fixtures/waitBeforeCaptureLocal",
        "test/fixtures/nodiffs",
        "test/e2e/latest",
        "test/e2e/next",
        "test/e2e/6.3.12",
        "test/e2e/5.3.21",
        "test/e2e/5.0.0",
        "test/e2e/undefined"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "semi": true,
                "parser": "babel",
                "printWidth": 100,
                "singleQuote": true,
                "trailingComma": "all",
                "bracketSpacing": false,
                "jsxBracketSameLine": true
            }
        ],
        "@typescript-eslint/no-empty-function": "off",
        "no-console": "off"
    }
}